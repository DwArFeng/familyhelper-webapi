        <dependency>
            <groupId>com.dwarfeng</groupId>
            <artifactId>settingrepo-sdk</artifactId>
        </dependency>
        <!-- Notify 组件 -->
        <dependency>
            <groupId>com.dwarfeng</groupId>
            <artifactId>notify-sdk</artifactId>
        </dependency>
        <!-- 程序自身引用 -->
        <dependency>
            <groupId>com.dwarfeng</groupId>
            <artifactId>familyhelper-webapi-stack</artifactId>

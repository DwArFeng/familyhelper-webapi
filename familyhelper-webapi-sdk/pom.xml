        <version>1.2.2.a</version>
    </parent>

    <artifactId>familyhelper-webapi-sdk</artifactId>
    <name>familyhelper-webapi-sdk</name>
    <url>http://maven.apache.org</url>

    <properties>
        <!-- 进行工程编码声明 -->

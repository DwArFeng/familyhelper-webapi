<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd"
>

    <dubbo:application name="familyhelper-api-web-node">
        <dubbo:parameter key="qos.enable" value="false"/>
    </dubbo:application>

    <!-- zookeeper注册中心 -->
    <dubbo:registry address="${dubbo.registry.zookeeper.address}"/>

    <!-- dubbo协议 -->
    <dubbo:protocol name="dubbo" port="${dubbo.protocol.dubbo.port}" host="${dubbo.protocol.dubbo.host}"
                    dispatcher="message" threadpool="fixed" threads="100"/>

    <!-- 默认使用dubbo协议 -->
    <dubbo:provider protocol="dubbo"/>

    <!-- 向注册中心注册暴露服务地址,注册消费者服务 -->
    <!-- acckeeper -->
    <dubbo:reference id="acckeeperLoginService" interface="com.dwarfeng.acckeeper.stack.service.LoginService"
                     retries="0" timeout="60000" check="false"/>
    <dubbo:reference id="acckeeperAccountMaintainService"
                     interface="com.dwarfeng.acckeeper.stack.service.AccountMaintainService"
                     retries="0" timeout="60000" check="false"/>
    <dubbo:reference id="acckeeperAccountOperateService"
                     interface="com.dwarfeng.acckeeper.stack.service.AccountOperateService"
                     retries="0" timeout="60000" check="false"/>
    <!-- rbac -->
    <dubbo:reference id="rbacPermissionLookupService"
                     interface="com.dwarfeng.rbacds.stack.service.PermissionLookupService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="rbacUserMaintainService" interface="com.dwarfeng.rbacds.stack.service.UserMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="rbacRoleMaintainService"
                     interface="com.dwarfeng.rbacds.stack.service.RoleMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="rbacPermissionMaintainService"
                     interface="com.dwarfeng.rbacds.stack.service.PermissionMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="rbacPexpMaintainService"
                     interface="com.dwarfeng.rbacds.stack.service.PexpMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="rbacPermissionGroupMaintainService"
                     interface="com.dwarfeng.rbacds.stack.service.PermissionGroupMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <!-- familyhelper-finance -->
    <dubbo:reference id="familyhelperFinanceAccountBookMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.AccountBookMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceAccountBookOperateService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.AccountBookOperateService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceBankCardMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.BankCardMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceFundChangeMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.FundChangeMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceBankCardOperateService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.BankCardOperateService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceBalanceOperateService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.BalanceOperateService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceFundChangeOperateService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.FundChangeOperateService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceUserMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.UserMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinancePoabMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.PoabMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceBankCardTypeIndicatorMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.BankCardTypeIndicatorMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceFundChangeTypeIndicatorMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.FundChangeTypeIndicatorMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceTotalBalanceHistoryMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.TotalBalanceHistoryMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperFinanceBankCardBalanceHistoryMaintainService"
                     interface="com.dwarfeng.familyhelper.finance.stack.service.BankCardBalanceHistoryMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <!-- familyhelper-clannad -->
    <dubbo:reference id="familyhelperClannadProfileMaintainService"
                     interface="com.dwarfeng.familyhelper.clannad.stack.service.ProfileMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperClannadProfileOperateService"
                     interface="com.dwarfeng.familyhelper.clannad.stack.service.ProfileOperateService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperClannadProfileTypeIndicatorMaintainService"
                     interface="com.dwarfeng.familyhelper.clannad.stack.service.ProfileTypeIndicatorMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperClannadPoprMaintainService"
                     interface="com.dwarfeng.familyhelper.clannad.stack.service.PoprMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperClannadUserMaintainService"
                     interface="com.dwarfeng.familyhelper.clannad.stack.service.UserMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <dubbo:reference id="familyhelperClannadNicknameMaintainService"
                     interface="com.dwarfeng.familyhelper.clannad.stack.service.NicknameMaintainService"
                     retries="3" timeout="60000" check="false"/>
    <!-- snowflake -->
    <dubbo:reference id="longIdService" interface="com.dwarfeng.sfds.stack.service.LongIdService" check="false"/>
</beans>
